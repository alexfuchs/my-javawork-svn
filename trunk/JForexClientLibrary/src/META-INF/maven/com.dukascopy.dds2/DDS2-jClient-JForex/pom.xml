                                    }
                                </option>
                                
                                <!-- use reflection -->
                                <option>
                                    -keep class
                                    com.dukascopy.charts.persistence.IndicatorBean                                    
                                </option>

                                <!-- serialized class -->
                                <option>
                                    -keep class
                                    com.dukascopy.charts.data.datacache.OrderHistoricalData
                                </option>
                            </options>
                            <libs>
